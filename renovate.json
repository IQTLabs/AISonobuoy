{
  "extends": [
    "config:base"
  ],
  "ignorePaths": [],
  "pip-compile": {
    "fileMatch": ["(^|/)requirements\\.in$"]
  },
  "packageRules": [
    {
      "matchPackagePatterns": ["^boto"],
      "groupName": "Update dependency botocore and boto3"
    }
  ],
  "timezone": "America/New_York",
  "schedule": [
    "after 10pm every weekday",
    "before 5am every weekday",
    "every weekend"
  ]
}
